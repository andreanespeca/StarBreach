      <profiles>
        <profile id="0982-2a03-b9ba-0483" name="Eggs (Brood Mother only)" hidden="false" typeId="d89f-2134-4e54-a502" typeName="Weapon">
          <characteristics>
            <characteristic name="Range" typeId="549e-ea38-4e69-a0d9">6</characteristic>
